{
  "name": "carbon-tutorial-nextjs",
  "version": "0.1.0",
  "private": true,
  "repository": "git@github.com:carbon-design-system/carbon-tutorial-nextjs.git",
  "bugs": "https://github.com/carbon-design-system/carbon-tutorial-nextjs/issues",
  "license": "Apache-2.0",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "ci-check": "yarn format:diff",
    "clean": "yarn cache clean && yarn install",
    "format": "prettier --write \"**/*.{js,md,scss}\"",
    "format:diff": "prettier --list-different \"**/*.{js,md,scss}\""
  },
  "dependencies": {
    "@carbon/icons-react": "^11.32.0",
    "@carbon/react": "^1.35.0",
    "axios": "^1.6.2",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.9",
    "framer-motion": "^10.16.16",
    "next": "^13.5.6",
    "ng2-dragula": "^5.0.1",
    "react": "18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "18.2.0",
    "react-dragula": "^1.1.17",
    "react-edit-text": "^5.1.1",
    "react-router-dom": "^6.20.1",
    "react-sticky-box": "^2.0.5",
    "react-use-websocket": "^4.5.0",
    "sass": "^1.66.1",
    "uuid": "^9.0.1",
    "ws": "^8.14.2",
    "yarn": "^1.22.21"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "7.5.0",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.42",
    "husky": "1.3.1",
    "lint-staged": "^15.2.0",
    "prettier": "^2.8.8"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --cache --write",
      "git add"
    ],
    "*.{scss,css}": [
      "prettier --cache --write",
      "git add"
    ],
    "*.md": [
      "prettier --cache --write",
      "git add"
    ]
  },
  "prettier": {
    "jsxBracketSameLine": true,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
